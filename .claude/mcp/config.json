{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/"],
      "env": {}
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "gitlab": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-gitlab"],
      "env": {
        "GITLAB_PERSONAL_ACCESS_TOKEN": "${GITLAB_TOKEN}",
        "GITLAB_API_URL": "${GITLAB_API_URL:-https://gitlab.com/api/v4}"
      }
    },
    "git": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-git"],
      "env": {}
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "env": {}
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "env": {}
    },
    "fetch": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-fetch"],
      "env": {}
    },
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"],
      "env": {}
    },
    "sqlite": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sqlite", "--db-path", "~/.claude/data/local.db"],
      "env": {}
    },
    "postgres": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-postgres"],
      "env": {
        "POSTGRES_CONNECTION_STRING": "${POSTGRES_CONNECTION_STRING}"
      }
    },
    "youtube-transcript": {
      "command": "npx",
      "args": ["-y", "@kimtaeyoon83/mcp-server-youtube-transcript"],
      "env": {}
    },
    "docker": {
      "command": "npx",
      "args": ["-y", "@chhabraamit/mcp-docker"],
      "env": {}
    },
    "kubernetes": {
      "command": "npx",
      "args": ["-y", "@flux159/mcp-server-kubernetes"],
      "env": {
        "KUBECONFIG": "${KUBECONFIG:-~/.kube/config}"
      }
    },
    "aws-cli": {
      "command": "npx",
      "args": ["-y", "@alexei-led/aws-mcp-server"],
      "env": {
        "AWS_PROFILE": "${AWS_PROFILE:-default}",
        "AWS_REGION": "${AWS_REGION:-us-east-1}"
      }
    },
    "time": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-time"],
      "env": {}
    },
    "web-search": {
      "command": "npx",
      "args": ["-y", "@pskill9/web-search"],
      "env": {}
    },
    "mdconvert": {
      "command": "npx",
      "args": ["-y", "@chrisgscott/mcp-mdconvert"],
      "env": {}
    },
    "aws-kb": {
      "command": "npx",
      "args": ["-y", "@makowski/mcp-server-aws-kb"],
      "env": {}
    },
    "hacker-news": {
      "command": "npx",
      "args": ["-y", "github:pskill9/hn-server"],
      "env": {}
    },
    "calculator": {
      "command": "npx",
      "args": ["-y", "github:pwh-pwh/cal-mcp"],
      "env": {}
    },
    "wikipedia": {
      "command": "npx",
      "args": ["-y", "github:future-audiences/wikimedia-enterprise-model-context-protocol"],
      "env": {}
    }
  }
}